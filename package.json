{
  "name": "website-pfarre-machstrasse",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "bundles": "ng build --stats-json && npx webpack-bundle-analyzer ./dist/Website-Pfarre-Machstrasse/stats.json",
    "start": "ng serve",
    "build": "ng build",
    "postbuild": "node postbuild.js",
    "build:prod": "ng build --prod",
    "postbuild:prod": "node postbuild.js",
    "lint:code": "ng lint",
    "lint:code:fix": "ng lint --fix",
    "lint:code:ci": "ng lint --format checkstyle --silent --force > analysis/code-lint.xml",
    "lint:style": "stylelint src/**/*.scss",
    "lint:style:ci": "stylelint --custom-formatter ./checkstyle src/**/*.scss -o analysis/style-lint.xml",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:ci": "npm run lint:code:ci && npm run lint:style:ci",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCi",
    "test": "ng test",
    "e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "update-angular": "ng update @angular/cli @angular/core --allow-dirty"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "~11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@angular/service-worker": "~11.2.14",
    "@ctrl/ngx-codemirror": "^4.1.0",
    "@fullcalendar/angular": "^5.5.0",
    "@fullcalendar/core": "^5.5.0",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/list": "^5.5.0",
    "@fullcalendar/timegrid": "^5.5.0",
    "bootstrap": "^5.0.0-beta2",
    "codemirror": "^5.60.0",
    "ng-gallery": "^5.0.0",
    "ngx-showdown": "^6.0.0",
    "rxjs": "~6.6.7",
    "showdown": "^1.9.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "~11.2.13",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/localize": "^11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/showdown": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.3.0",
    "codelyzer": "^6.0.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-rxjs": "^3.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "~7.0.0",
    "purgecss": "^4.0.2",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.11.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.5",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
